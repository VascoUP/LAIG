<dsx>

    <scene root="root" axis_length="2" />
        
    <views default="p1" >

        <perspective id="p1" near="0.4" far="500" angle="10">

            <from x="50" y="50" z="50" />
            <to x="0" y="10" z="0" />

        </perspective>

        <perspective id="p2" near="0.4" far="200" angle="30">

            <from x="-60" y="60" z="0" />
            <to x="-5" y="20" z="0" />

        </perspective>        

        <perspective id="d1" near="0.2" far="400" angle="30">

            <from x="50" y="50" z="40" />
            <to x="0" y="15" z="5" />

        </perspective>

    </views>
    

    <illumination doublesided="0" local="1" >
    
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />
        <background r="0" g="0" b="0" a="1.0" />
        
    </illumination>
    
    
    <lights>

        <omni id="l1" enabled="1" >
            <location x="15" y="15" z="15" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.4" g="0.3" b="0.5" a="1.0" />
        </omni>

        <omni id="l3" enabled="1" >
            <location x="-15" y="-20" z="20" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </omni>
        
        <spot id="l2" enabled="1" angle="1.47" exponent="10">
            <target x="0" y="0" z="0" />
            <location x="-30" y="30" z="-30"/>
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.7" g="0.4" b="0.2" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </spot>

        <spot id="4" enabled="1" angle="1.47" exponent="10">
            <target x="0" y="0" z="0" />
            <location x="30" y="-30" z="30"/>
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.7" g="0.4" b="0.2" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </spot>

        <omni id="l5" enabled="1" >
            <location x="-30" y="-30" z="-30" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </omni>

    </lights>
        
    <textures>
        <texture id="t1" file="resources/Inox.jpg" length_s="1" length_t="1" />
    </textures>
    
    <materials>

        <!-- Brown diffuse -->
        <material id="w1" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0.3" g="0.3" b="0.3" a="1" />
            <diffuse r="0.9" g="0.3" b="0.4" a="1" />
            <specular r="0.2" g="0.2" b="0.2" a="1" />
            <shininess value="1" />
        </material>    

        <material id="tt" >
            <emission r="0" g="0" b="0" a="1" />
            <ambient r="0.3" g="0.3" b="0.3" a="1" />
            <diffuse r="0.3" g="0.3" b="0.3" a="1" />
            <specular r="0.9" g="0.9" b="0.9" a="1" />
            <shininess value="1" />
        </material>

    </materials>
    
    <transformations>

    </transformations>
    
    <animations>
        <animation id="c1" span="2.5" type="circular" 
				centerx="0" centery="0" centerz="0"
                radius="5" startang="0" rotang="90" />

        <animation id="c2" span="5" type="circular" 
				centerx="0" centery="0" centerz="0"
                radius="5" startang="90" rotang="-180" />
    </animations>

    <primitives>

        <primitive id="vehicle">
            <vehicle />
        </primitive>

    </primitives>
       
    <components>
        
        <!-- Root Element -->
        <component id="root">        
            <transformation>
            </transformation>
            <animation>
                <animationref id="c1" />
                <animationref id="c2" />
            </animation>
            <materials>
                <material id="w1" />
                <material id="tt" />
            </materials>
            <texture id="t1" />
            <children>
                <primitiveref id="vehicle" />
            </children>       
        </component>


    </components>

</dsx>